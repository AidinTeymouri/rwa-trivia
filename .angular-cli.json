{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "trivia"
  },
  "apps": [
    {
      "name": "trivia",
      "root": "src",
      "outDir": "dist/trivia",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles/rwa-trivia.theme.scss",
        "styles/styles.scss",
        "styles/global.scss",
        "styles/light.theme.scss",
        "styles/dark.theme.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/prod/environment.ts",
      "environments": {
        "dev": "environments/prod/environment.ts",
        "prod": "environments/prod/environment.prod.ts"
      }
    },
    {
      "name": "trivia-dev",
      "root": "src",
      "outDir": "dist/trivia-dev",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "dev-app",
      "styles": [
        "styles/rwa-trivia.theme.scss",
        "styles/styles.scss",
        "styles/global.scss",
        "styles/light.theme.scss",
        "styles/dark.theme.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.dev.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
        "inlineTemplate": false,
        "spec": true
      }
  }
}
