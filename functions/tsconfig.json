{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "target": "es6",
    "outDir": "../server",
    "rootDirs": [
      ".",
      "../src/app/model"
    ],
    "paths": {
      "model": ["../src/app/model"]
    },
    "baseUrl": ".",
    "lib": [
      "es2016"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "module": "commonjs",
    "types": ["node"]
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
